{
  "npmScope": "flight-workspace",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "booking-domain": { "tags": ["domain:booking", "type:domain-logic"] },
    "booking": { "tags": ["domain:booking", "type:app"] },
    "booking-feature-search": { "tags": ["domain:booking", "type:feature"] },
    "booking-feature-checkout": { "tags": ["domain:booking", "type:feature"] },
    "boarding-domain": { "tags": ["domain:boarding", "type:domain-logic"] },
    "boarding": { "tags": ["domain:boarding", "type:app"] },
    "boarding-feature-manage": { "tags": ["domain:boarding", "type:feature"] },
    "luggage-domain": { "tags": ["domain:luggage", "type:domain-logic"] },
    "luggage": { "tags": ["domain:luggage", "type:app"] },
    "luggage-feature-checkin": { "tags": ["domain:luggage", "type:feature"] },
    "luggage-feature-report-loss": {
      "tags": ["domain:luggage", "type:feature"]
    },
    "shared-util-auth": { "tags": ["domain:shared", "type:util"] },
    "shared-util-logger": { "tags": ["domain:shared", "type:util"] }
  }
}
